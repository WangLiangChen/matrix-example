<?xml version="1.0" encoding="UTF-8"?>
<!-- output 代码输出路径,默认为当前项目的src/main/java -->
<entities output="" base-package="com.sintrue.matrix.example" author="Liangchen.Wang">
    <entity>
        <!-- 多数据源时,指定数据源，默认为primary
        <datasource></datasource>
         -->
        <table-name>staff</table-name>
        <!-- 指定实体类名,默认下划线转驼峰
        <entity-name>AuthorizationSubject</entity-name>
         -->
        <!-- 限界上下文包名 -->
        <context-package>hr</context-package>
        <!-- 聚合包名 -->
        <aggregate-package>staff</aggregate-package>
        <!-- 是否聚合根 默认false -->
        <aggregate-root>true</aggregate-root>
        <!-- 指定版本列名，用于乐观锁 -->
        <column-version>version</column-version>
        <!-- 指定状态列名;指定状态列是否使用框架提供的常量类ConstantEnum及其子类 -->
        <!-- 自动生成和状态查询相关的代码 -->
        <column-state useConstantEnum="false">state</column-state>
        <!-- 指定逻辑删除的列和值 -->
        <column-markdelete value="DELETED">state</column-markdelete>
        <!-- 指定自动转换对象和Json的列 -->
        <column-json>staff_settings</column-json>
        <!-- 列名是否转驼峰风格属性名,默认为true
        <camel-case>true</camel-case>
         -->
    </entity>
</entities>
